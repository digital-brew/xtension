{
  "name": "digital-brew/xtension",
  "description": "Simply extend WP Admin.",
  "keywords": [
    "themosis",
    "plugin"
  ],
  "authors": [
    {
      "name": "Rafal Pajak",
      "email": "rafal@digitalbrew.io",
      "homepage": "https://digitalbrew.io"
    }
  ],
  "autoload": {
    "psr-4": {
      "DigitalBrew\\Xtension\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "type": "wordpress-plugin",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "git@github.com:digital-brew/hooks"
    }
  ],
  "require": {
    "php": "^8.1",
    "composer/installers": "^2.2",
    "digital-brew/hooks": "dev-main"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
